async function fetchproducts() {
  const response = await fetch ('https://fa25-cs300-tomaszswiderski.github.io/TomaszSwiderski.github.io/products.json');
  const products = await response.json();
  console.log("First 3 products:", products);
}
async function fetchUsers() {
   const response = await fetch ('https://fa25-cs300-tomaszswiderski.github.io/TomaszSwiderski.github.io/Users.json');
   const Users = await response.json();
   console.log("First 3 Users:", Users);  
}
async function fetchOrders() {
  const response = await fetch ('https://fa25-cs300-tomaszswiderski.github.io/TomaszSwiderski.github.io/Orders.json');
  const Orders = await response.json();
  console.log("First 3 Orders:", Orders);
}

fetchproducts();
fetchUsers();
fetchOrders();