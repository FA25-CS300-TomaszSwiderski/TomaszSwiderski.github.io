// Fetch Users
async function fetchUsers() {
  const res = await fetch("https://<your-username>.github.io/<repo-name>/api/users.json");
  const data = await res.json();
  console.log("Users:", data);
  return data;
}

// Fetch Products
async function fetchProducts() {
  const res = await fetch("https://<your-username>.github.io/<repo-name>/api/products.json");
  const data = await res.json();
  console.log("Products:", data);
  return data;
}
// Example: filter users by age > 30
async function getOlderUsers() {
  const users = await fetchUsers();
  return users.filter(u => u.age > 30);
}

// Example: find products cheaper than $200
async function getCheapProducts() {
  const products = await fetchProducts();
  return products.filter(p => p.price < 200);
}
